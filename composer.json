{
    "name": "chipulaja/algo-sudoku-generator",
    "description": "a library for generate sudoku",
    "keywords": ["sudoku generator", "puzzle generator", "sudoku", "puzzle"],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Chipulaja\\Algo\\Sudoku\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "scrutinizer/ocular": "^1.8"
    },
    "authors": [
        {
            "name": "chipulaja",
            "email": "chipul.aja@gmail.com"
        }
    ],
    "scripts": {
        "cs-check": "phpcs --standard=PSR12 -n src",
        "cs-fix": "phpcbf --standard=PSR12 -n src",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html build"
    },
    "license": "MIT"
}
